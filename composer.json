{
    "name": "jgabboud/laravel-subscriptions",
    "description": "A Laravel package that will allow you to have dynamic subscriptions with free trials and discounts, enjoy!",    
    "type": "library",
    "license": "MIT",
    "keywords":
        [
            "laravel",
            "subscriptions",
            "subscribe",
            "plan",
            "package",
            "model"
        ],
    "authors": [
        {
            "name": "Joseph Abboud",
            "email": "jgabboud99@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jgabboud\\Subscriptions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jgabboud\\Subscriptions\\SubscriptionServiceProvider"
            ]
        }
    },
    "require": {        
        "php": "^8.0.0",
        "illuminate/console": "^9.0.0 || ^10.0.0",
        "illuminate/database": "^9.0.0 || ^10.0.0",
        "illuminate/support": "^9.0.0 || ^10.0.0",
        "spatie/eloquent-sortable": "^4.0.0",
        "spatie/laravel-sluggable": "^3.4.0",
        "spatie/laravel-translatable": "^6.0"
    },    
    "minimum-stability": "dev",
    "prefer-stable": true
}
